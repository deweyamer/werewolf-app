{
  "name": "werewolf-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "tsx src/test/runE2ETests.ts all",
    "test:e2e:smoke": "tsx src/test/runE2ETests.ts smoke",
    "test:e2e:critical": "tsx src/test/runE2ETests.ts critical",
    "test:e2e:important": "tsx src/test/runE2ETests.ts important"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "proper-lockfile": "^4.1.2",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.7",
    "@types/uuid": "^9.0.7",
    "@vitest/ui": "^4.0.16",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  }
}
